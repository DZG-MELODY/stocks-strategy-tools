    "cross-env": "^7.0.3",
    "csv-parse": "^5.5.0",
    "electron": "^26.1.0",
    "electron-builder": "^24.6.4",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "iconv-lite": "^0.6.3",
    "lowdb": "^6.0.1",
    "naive-ui": "^2.34.4",
    "node-fetch": "^3.3.2",
    "postcss": "^8.4.29",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "unplugin-auto-import": "^0.16.6",
