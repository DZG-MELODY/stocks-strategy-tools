{
  "name": "stocks-strategy-tools",
  "version": "0.0.1",
  "main": "dist-electron/main/index.js",
  "description": "awesome tools for make money by stocks",
  "author": "melody-dzg",
  "license": "MIT",
  "private": true,
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ],
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/",
